# baseURI: https://bluebikes.com/system-data/rdf/ontology#

@base          <https://bluebikes.com/system-data/rdf/ontology#> .
@prefix bike_ontology: <https://bluebikes.com/system-data/rdf/ontology#> .
@prefix metadata: <http://topbraid.org/metadata#> .
@prefix teamwork: <http://topbraid.org/teamwork#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix graphql: <http://datashapes.org/graphql#> .

<urn:x-evn-master:bike_ontology/Station-stationLatitude>
        rdf:type     sh:PropertyShape ;
        sh:datatype  xsd:decimal ;
        sh:name      "latitude" ;
        sh:path      <urn:x-evn-master:bike_ontology/latitude> ;
        sh:maxCount  1 .

bike_ontology:District
        rdfs:label       "District" ;
        rdf:type         owl:Class ;
        rdf:type         sh:NodeShape ;
        rdfs:subClassOf  skos:Concept .

bike_ontology:Rideable
        rdfs:label       "Rideable" ;
        rdf:type         owl:Class ;
        rdf:type         sh:NodeShape ;
        rdfs:subClassOf  skos:Concept .

bike_ontology:Rider  rdfs:label  "Rider" ;
        rdf:type         owl:Class ;
        rdf:type         sh:NodeShape ;
        rdfs:subClassOf  skos:Concept .

bike_ontology:Station  rdfs:label  "Station" ;
        rdf:type         owl:Class ;
        rdf:type         sh:NodeShape ;
        rdfs:subClassOf  skos:Concept ;
        sh:property      bike_ontology:numDocks ;
        sh:property      bike_ontology:latitude ;
        sh:property      bike_ontology:dateEstablished ;
        sh:property      bike_ontology:longitude ;
        sh:property      bike_ontology:district ;
        sh:property      bike_ontology:stationID ;
        sh:property      bike_ontology:label .

bike_ontology:Trip  rdfs:label  "Trip" ;
        rdf:type         owl:Class ;
        rdf:type         sh:NodeShape ;
        rdfs:subClassOf  owl:Thing ;
        sh:property      bike_ontology:endedAt ;
        sh:property      bike_ontology:endStation ;
        sh:property      bike_ontology:rideableType ;
        sh:property      bike_ontology:startedAt ;
        sh:property      bike_ontology:startStation ;
        sh:property      bike_ontology:tripDuration ;
        sh:property      bike_ontology:tripID ;
        sh:property      bike_ontology:label .

bike_ontology:endedAt  rdf:type  sh:PropertyShape ;
        sh:datatype  xsd:dateTime ;
        sh:name      "endedAt" ;
        sh:path      bike_ontology:endedAt .

bike_ontology:endStation
        rdf:type  sh:PropertyShape ;
        sh:class  bike_ontology:Station ;
        sh:name   "endStation" ;
        sh:path   bike_ontology:endStation ;
        sh:node   bike_ontology:Station .

bike_ontology:rideableType
        rdf:type  sh:PropertyShape ;
        sh:class  bike_ontology:Rideable ;
        sh:name   "rideableType" ;
        sh:path   bike_ontology:rideableType ;
        sh:node   bike_ontology:Rideable .

bike_ontology:startedAt
        rdf:type     sh:PropertyShape ;
        sh:datatype  xsd:dateTime ;
        sh:name      "startedAt" ;
        sh:path      bike_ontology:startedAt .

bike_ontology:startStation
        rdf:type  sh:PropertyShape ;
        sh:class  bike_ontology:Station ;
        sh:name   "startStation" ;
        sh:path   bike_ontology:startStation ;
        sh:node   bike_ontology:Station .

bike_ontology:tripDuration
        rdf:type     sh:PropertyShape ;
        sh:datatype  xsd:integer ;
        sh:name      "tripDuration" ;
        sh:path      bike_ontology:tripDuration .

bike_ontology:tripID  rdf:type  sh:PropertyShape ;
        sh:datatype  xsd:string ;
        sh:name      "tripID" ;
        sh:path      bike_ontology:tripID .

bike_ontology:numDocks
        rdf:type     sh:PropertyShape ;
        sh:datatype  xsd:integer ;
        sh:name      "numDocks" ;
        sh:path      bike_ontology:numDocks .

bike_ontology:latitude
        rdf:type     sh:PropertyShape ;
        sh:datatype  xsd:decimal ;
        sh:name      "latitude" ;
        sh:path      bike_ontology:latitude ;
        sh:maxCount  1 .

bike_ontology:dateEstablished
        rdf:type     sh:PropertyShape ;
        sh:datatype  xsd:date ;
        sh:name      "dateEstablished" ;
        sh:path      bike_ontology:dateEstablished .

bike_ontology:longitude
        rdf:type     sh:PropertyShape ;
        sh:datatype  xsd:decimal ;
        sh:name      "longitude" ;
        sh:path      bike_ontology:longitude ;
        sh:maxCount  1 .

bike_ontology:district
        rdf:type  sh:PropertyShape ;
        sh:class  bike_ontology:District ;
        sh:name   "district" ;
        sh:path   bike_ontology:district ;
        sh:node   bike_ontology:District .

bike_ontology:stationID
        rdf:type     sh:PropertyShape ;
        sh:datatype  xsd:string ;
        sh:name      "stationID" ;
        sh:path      bike_ontology:stationID ;
        sh:maxCount  1 .

bike_ontology:label  rdf:type  sh:PropertyShape ;
        sh:datatype  xsd:string ;
        sh:name      "label" ;
        sh:path      rdfs:label ;
        sh:maxCount  1 ;
        sh:minCount  1 .

bike_ontology:  graphql:publicClass  owl:Thing ;
        owl:imports          <http://datashapes.org/graphql> ;
        owl:imports          <http://topbraid.org/skos.shapes> ;
        rdf:type             owl:Ontology ;
        <http://topbraid.org/swa#defaultNamespace>  "https://bluebikes.com/system-data/rdf/ontology#" ;
        rdfs:label           "Bike Ontology" ;
        metadata:status      metadata:UnderDevelopmentStatus .
